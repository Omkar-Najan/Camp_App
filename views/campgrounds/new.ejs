<%- include("../partials/header") %>
    <div class="container">
        <div class="row">
            <h1 style="text-align: center;">Create a New Caomground</h1>
            <div style="width:40%;margin:25px auto;">
                <form action="/campgrounds" method="POST">
                    <div class="form-group">
                        <input type="text" placeholder="name" class="form-control"  id="" name="name">
                    </div>
                    <div class="form-group">
                        <input type="number" placeholder="price"  min="0.01" step="0.01" class="form-control"  id="" name="price">
                    </div>
                    <div class="form-group">
                        <input type="text" name="img" id="" class="form-control" placeholder="img url">
                    </div> 
                    <div class="form-group">
                        <input type="text" name="description" id="" class="form-control" placeholder="description">
                    </div>
                    <div class="form-group">
                        <button class="btn btn-primary btn-lg btn-default btn-block">Submit</button>
                    </div>
                    
                </form>
                <a class="btn btn-lg btn-block" href="/campgrounds">Go Back</a>
            </div>
            
         
        </div>
    </div>
<%- include("../partials/footer") %>